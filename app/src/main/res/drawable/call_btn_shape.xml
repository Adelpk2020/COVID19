<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:left="40dp" android:top="0dp" android:bottom="0dp">

        <shape android:shape="oval">
            <solid android:color="@color/navy"/>
            <size android:width="60dp" android:height="40dp"></size>

        </shape>

    </item>

    <item android:left="20dp" android:right="20dp" android:top="3dp" android:bottom="3dp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/navy"/>
            <size android:width="50dp" android:height="10dp"></size>
        </shape>

    </item>

    <item android:right="40dp">
        <shape android:shape="oval" >
            <solid android:color="#70fbf0"/>
            <size android:width="60dp" android:height="50dp"></size>

        </shape>

    </item>

</layer-list>